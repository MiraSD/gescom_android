<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >
    <TextView
        android:id="@+id/partnerid"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FF8A80"
        android:layout_weight="2"/>
    <TextView
        android:id="@+id/partnername"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />
    <TextView
        android:id="@+id/partnerlastname"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />
    <TextView
        android:id="@+id/partnermatFiscal"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />
    <TextView
        android:id="@+id/partneradress"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />
    <TextView
        android:id="@+id/partnertel"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />
    <TextView
        android:id="@+id/partnertype"
        android:textSize="25sp"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_weight="2"
        />

</LinearLayout>
